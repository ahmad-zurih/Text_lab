---
batch_connect:
  template: "basic"


script:
  # Source these scripts before and after the main script
  before_file: "template/before.sh.erb"
  after_file: "template/after.sh"

  
  native:
    - "--partition=<%= partition %>"
    - "--qos=<%= qos %>"
    - "--gres=<%= gpu %><%= gpu_number %>"
    - "--time=<%= time %>:00:00"
    - "--mem-per-cpu=8G"
    - "--cpus-per-task=4"
 
